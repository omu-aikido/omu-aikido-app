---
import EditRecordForm from "@/rr7/components/component/EditRecordForm"
import { default as Layout } from "@/src/layouts/layout.astro"
const prePage = new URL(Astro.url).searchParams.get("page")
const redirectTo = prePage ? `/apps/record/list?page=${prePage}` : "/apps/record/list"
const { id } = Astro.params
---

<Layout title="編集">
  <EditRecordForm id={id!} redirectTo={redirectTo} />
</Layout>

